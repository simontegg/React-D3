{
  "name": "react_D3",
  "author": "simontegg",
  "version": "0.0.0",
  "license": "AGPLv3",
  "main": "index.js",
  "scripts": {
    "develop": "beefy index.js:bundle.js --live --index=index.html",
    "build": "mkdir -p dist && cp index.html dist/ && browserify index.js -o dist/bundle.js -d",
    "deploy": "npm run build && gulp deploy"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/simontegg/react_d3"
  },
  "dependencies": {
    "d3": "^3.4.11",
    "d3-builder": "^0.2.0",
    "d3-scale": "0.0.0",
    "domready": "*",
    "lodash": "^2.4.1",
    "mouse-position": "^1.0.0",
    "numeric": "^1.2.6",
    "react": "^0.11.1",
    "reactify": "^0.14.0",
    "uuid": "^1.4.1"
  },
  "devDependencies": {
    "beefy": "^2.1.0",
    "gulp": "^3.8.7",
    "gulp-gh-pages": "^0.3.3"
  }
}
